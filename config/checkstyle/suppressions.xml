<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <suppress checks="MissingJavadocMethod" files=".*/BrokerClientTests.java" />
    <suppress checks="OverloadMethodsDeclarationOrder" files=".*/Consumer.java|
              |.*/ConsumerImpl.java" />
